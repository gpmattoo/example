<!DOCTYPE hibernate-configuration PUBLIC
	"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<hibernate-configuration>
	<session-factory>
		<property name="hibernate.dialect">org.hibernate.dialect.MySQL5InnoDBDialect</property>
		<property name="hibernate.showSql">true</property>
		<property name="hibernate.formatSql">true</property>
		<mapping class="org.openapplicant.common.domain.PersonName"/>
		<mapping class="org.openapplicant.user.service.impl.domain.User"/>
    </session-factory>
</hibernate-configuration>
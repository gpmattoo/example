import grails.test.*

class UserIntegrationTests extends GrailsUnitTestCase {
    protected void setUp() {
        super.setUp()
    }

    protected void tearDown() {
        super.tearDown()
    }

    void testSomething() {
		def u = new User(userId:'foo')
		assertNotNull u.save(flush:true)

		def found = User.get(u.id)
		assertNotNull found
    }
}
